{
  "name": "mock-weibo-api",
  "version": "0.0.0",
  "description": "Mock Weibo API.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "example": "src/demo",
    "test": "src/test"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "sh -c 'rm -rf dist out temp'",
    "debug": "node --inspect-brk -r ts-node/register src/main/index.ts",
    "demo": "cross-env MODE=DEV node -r ts-node/register src/demo/index.ts",
    "dev": "cross-env MODE=DEV node -r ts-node/register src/main/index.ts",
    "docker": "docker build -t production/mock-weibo-api .",
    "pack": "webpack --config webpack.prod.js",
    "prepublishOnly": "npm run clean && npm run build",
    "prod": "cross-env MODE=PROD node dist",
    "start": "node dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "watch": "tsc -w"
  },
  "keywords": [
    "typescript",
    "project"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://blog.don.red/)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@iinfinity/logger": "^0.3.4",
    "@rester/core": "^0.10.24",
    "mongodb": "^3.6.2"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.14",
    "@types/mongodb": "^3.5.27",
    "@types/node": "14.11.2",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cross-env": "7.0.2",
    "eslint": "7.10.0",
    "jasmine": "3.6.1",
    "jasmine-console-reporter": "3.1.0",
    "superagent": "^6.1.0",
    "ts-loader": "8.0.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/mock-weibo-api.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/mock-weibo-api/issues"
  },
  "homepage": "https://github.com/DevinDon/mock-weibo-api",
  "optionalDependencies": {}
}
